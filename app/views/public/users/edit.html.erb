<h2>プロフィール編集</h2>
  <%= form_with model:@user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <label for="inputWork_out">Name</label>
  <%= f.text_field :work_out, autofocus: true, id:"inputWork_out", placeholder:"運動頻度"%>
  <label for="user_introduction">Introduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"inputIntroduction", placeholder:"自己紹介"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
  <%= link_to "退会する", users_quit_path, class: "btn btn-md btn-danger" %>
<% end %>