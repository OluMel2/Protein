<h1>Admin::Users#show</h1>
<div class="container">
  <strong>名前</strong>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image, width: "100×100" %>
  <strong>運動頻度</strong>
  <%= @user.work_out %>
  <strong>自己紹介</strong>
  <%= @user.introduction %>
  <p><%= link_to "プロフィール編集", edit_admin_user_path(@user.id) %></p>
</div>