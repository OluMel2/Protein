<div class="container">
  <div class="row mt-4">
    <div class="col-lg-12">
      <div class="text-center pb-5">
        <div class="pt-5">
          <h2 class="mb-4">タグ編集</h2>
          <%= form_with model:@tag,url:admin_tag_path(@tag) do |f| %>
          <p><%= f.text_field :name %></p>
          <div>
            <%= f.submit "変更を保存", class: "btn btn-success"%>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>